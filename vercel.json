{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "requirement": "requirements.txt",
        "maxDuration": 20
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
      "maxDuration": 15
    }
  ]
}
